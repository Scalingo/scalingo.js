{
  "name": "scalingo",
  "version": "0.5.0",
  "description": "Wrapper for the Scalingo API",
  "main": "dist/scalingo.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "Scalingo/scalingo.js",
  "author": "Jonathan Hurter <john@scalingo.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn run lint:js --quiet && tsc && node bundle.js",
    "docs": "typedoc src/index.ts",
    "lint": "yarn run lint:ts && yarn run lint:js",
    "lint:js": "eslint --ext .js,.ts ./src ./test",
    "lint:ts": "tsc --noEmit",
    "test": "ts-mocha --require @babel/register --recursive --paths -p test/tsconfig.test.json test/*.js test/**/*.test.js"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "isomorphic-ws": "4.x",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/register": "7.x",
    "@types/node": "17.x",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "axios-mock-adapter": "^1.20.0",
    "chai": "^4.3.4",
    "core-js": "3.x",
    "esbuild": "^0.14.6",
    "esdoc": "1.x",
    "esdoc-ecmascript-proposal-plugin": "1.x",
    "esdoc-standard-plugin": "1.x",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "rosie": "^2.1.0",
    "sinon": "^12.0.1",
    "ts-mocha": "^8.0.0",
    "tsconfig-paths": "^3.12.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
